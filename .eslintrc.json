{
  "extends": "standard-with-typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/quotes": [
      "error",
      "double",
      { "allowTemplateLiterals": true }
    ],
    "@typescript-eslint/camelcase": ["off"],
    "@typescript-eslint/comma-spacing": "off"
  }
}
